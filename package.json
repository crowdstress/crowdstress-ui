{
  "name": "crowdstress-ui",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack.dev.ts",
    "build:dev": "rm -rf ./build && webpack --config webpack.dev.ts",
    "build:prod": "rm -rf ./build && webpack --config webpack.prod.ts",
    "test:unit": "jest --testPathPattern=tests/unit --collect-coverage",
    "lint": "eslint --no-fix ./src",
    "start": "concurrently \"npm run lint\" \"npm run dev\"",
    "svgo": "svgo --pretty --indent=2 --multipass --enable removeDimensions --enable=inlineStyles --config '{ \"plugins\": [ { \"inlineStyles\": { \"onlyMatchedOnce\": false } }] }' -f"
  },
  "author": "Evgenii Kozlov",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@svgr/webpack": "^5.4.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.27.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.3.0",
    "svgo": "^1.3.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/reselect": "^2.2.0",
    "i18next": "^19.8.2",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.3"
  },
  "jest": {
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "isolatedModules": true
      }
    }
  }
}
